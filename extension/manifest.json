{
  "name": "Clear radio buttons",
  "version": "1.0.0",
  "description": "Clears all radio buttons on a page",
  "homepage_url": "https://github.com/brenopolanski/clear-radio-buttons",
  "manifest_version": 2,
  "minimum_chrome_version": "46",
  "applications": {
    "gecko": {
      "id": "breno.polanski@gmail.com",
      "strict_min_version": "45.0"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icon-20.png"
  },
  "icons": {
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
